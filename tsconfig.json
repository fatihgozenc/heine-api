{
	"compilerOptions": {
		"module": "esnext",
		"target": "es2019",
		"moduleResolution": "node",
		"allowJs": false,
		"strict": true,
		"isolatedModules": false,
		"esModuleInterop": true,
		"removeComments": true,
		"strictNullChecks": false,
		"skipLibCheck": true,
		"noUnusedLocals": true,
		"noUnusedParameters": true,
	},
	"include": [
		"./**/*.ts"
	],
	"exclude": [
		"node_modules/**/*",
		".serverless/**/*",
		".webpack/**/*",
		"_warmup/**/*",
		".vscode/**/*"
	]
}